                ////control que este dentro de la vision.
                if(x &gt; view_xview[0] &amp;&amp; x &lt; (view_xview[0] + view_wview[0]-5))
                    if(y &gt; view_yview[0] &amp;&amp; y &lt; (view_yview[0] + view_hview[0] + 5))
                        selected = true;
        }        
    }
    if (instance_exists(instV))  
    {        
