            <string>///////////patrullar//////
if(patrullar)
{    
    if(alarm[1]&lt; 0)
        if(vel &lt; velMovi)
        {
            vel = vel + aceleracion;
            alarm[1] = 15;
        }
    if(origx == x &amp;&amp; origy == y &amp;&amp; (destx != 0 || desty !=0))
    {        
        movin = true;        
    }
    if(volver)
    {
        mp_potential_step(origx,origy,vel,false);        
        if(origx == x &amp;&amp; origy == y)
        {
            volver = false;
        }
    }
    if(movin == true)
    {    
        mp_potential_step(destx,desty,vel,false);        
        if (distance_to_point(destx,desty) &lt; 60 &amp;&amp; !place_free(destx,desty))
        {
            movin = false;
            volver = true;        
        }
        if(destx == x &amp;&amp; desty == y)
        {
            movin = false;
            if(patrullar)
                volver = true;
        }
    }    
}
else
{
    ///////movimiento simple/////////
    if(movin == true)
    {       
        if(alarm[1]&lt; 0)
            if(vel &lt; velMovi)
            {
                vel = vel + aceleracion;
                alarm[1] = 15;
            }            
        mp_potential_step(destx,desty,vel,false);        
        if ((distance_to_point(destx,desty) &lt; 25 &amp;&amp; !place_free(destx,desty)) || (destx == x &amp;&amp; desty == y))        
        {
            movin = false;
            destx = 0;
            desty = 0;
        }        
    }
    else
    {
        vel = 0;
    }
}
/////////Atacar/////////
if(distance_to_object(obj_unit_enemigo) &lt; (vision * 230))
{
    switch(estado)
    {
        case "defensa":
        break;
        case "ataque":
            near = instance_nearest(x,y,obj_unit_enemigo);
            movin = false;
            patrullar = false;
            volver = false;
            if(objetivo == 0)
                objetivo = near;
            if(distance_to_object(obj_unit_enemigo)&gt;= alcance)
                mp_potential_step(near.x,near.y,velMovi,false);
            else
            {
                //animacion atacar
                if(near.vida &gt; 0)
                {
                    if(alarm[0] &lt; 0)
                    {
                        if(vel_vuelo == 0)
                        {
                            near.vida = near.vida - damage;
                            alarm[0] = vel_ataque;
                        }
                        else
                        {                            
                            flecha = instance_create(x,y,obj_flecha);
                            flecha.sprite_index = spr_flecha;
                            flecha.objetivoX = near.x;
                            flecha.objetivoY = near.y;
                            flecha.vel = vel_vuelo;
                            flecha.damage = damage;
                            flecha.objetivo = near;
                            alarm[0] = vel_ataque;
                        }
                    }
                }
            }
        break;
        case "uir":
        break;
    }    
}
</string>
